{
  "name": "remix-app",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix dev",
    "start": "remix-serve build/index.js",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "seed": "prisma db seed",
    "format": "prettier -c '**/*.ts'",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint app/**/*.{ts,tsx}",
    "test": "vitest test --watch",
    "test:run": "vitest test --run",
    "typecheck": "tsc",
    "validate": "run-p test:run lint typecheck"
  },
  "dependencies": {
    "@conform-to/react": "0.9.0",
    "@conform-to/zod": "0.9.0",
    "@prisma/client": "5.5.2",
    "@radix-ui/react-accordion": "1.1.2",
    "@radix-ui/react-alert-dialog": "1.0.5",
    "@radix-ui/react-aspect-ratio": "1.0.3",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-collapsible": "1.0.3",
    "@radix-ui/react-context-menu": "2.1.5",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-hover-card": "1.0.7",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-menubar": "1.0.4",
    "@radix-ui/react-navigation-menu": "1.1.4",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-progress": "1.0.3",
    "@radix-ui/react-radio-group": "1.1.3",
    "@radix-ui/react-scroll-area": "1.0.5",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-separator": "1.0.3",
    "@radix-ui/react-slider": "1.1.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-switch": "1.0.3",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-toast": "1.1.5",
    "@radix-ui/react-toggle": "1.0.3",
    "@radix-ui/react-tooltip": "1.0.7",
    "@remix-run/node": "2.1.0",
    "@remix-run/react": "2.1.0",
    "@remix-run/serve": "2.1.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "cmdk": "0.2.0",
    "date-fns": "2.30.0",
    "isbot": "3.7.0",
    "lucide-react": "0.290.0",
    "react": "18.2.0",
    "react-day-picker": "8.9.1",
    "react-dom": "18.2.0",
    "remix-auth": "3.6.0",
    "remix-auth-google": "1.3.0",
    "remix-auth-oauth2": "1.11.0",
    "remix-island": "0.1.2",
    "remix-utils": "7.1.0",
    "tailwind-merge": "1.14.0",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.1",
    "ts-pattern": "5.0.5",
    "zod": "3.22.4",
    "zodix": "0.4.4"
